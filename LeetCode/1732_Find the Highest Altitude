# 6/8/24 attempt 1
class Solution(object):
    def largestAltitude(self, gain):
        """
        :type gain: List[int]
        :rtype: int
        """
        
        curr_alt = 0
        max = 0
        
        for x in gain:
            curr_alt += x
            if (curr_alt > max):
                max = curr_alt

        return max
